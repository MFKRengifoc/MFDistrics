        <item name="mf_button_bg">@drawable/mf_button_primary_selector_but</item>
        <item name="mf_button_color_text">?attr/onPrimary</item>
    </style>
    <style name="MFButtonTextIcon.small" parent="MFButtonTextIcon">
        <item name="android:layout_width">@dimen/mf_button_width_s</item>
        <item name="android:layout_height">@dimen/mf_button_height_s</item>
        <item name="mf_button_text_size">@dimen/mf_button_text_s</item>
    </style>
    <style name="MFButtonTextIcon.medium" parent="MFButtonTextIcon">
        <item name="android:layout_width">@dimen/mf_button_width_m</item>
